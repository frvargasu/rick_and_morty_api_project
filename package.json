{
  "name": "rick_and_morty_api_project",
  "version": "1.0.0",
  "description": "Rick and Morty API Project - Complete REST API with Express.js",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js"
  },
  "keywords": ["rick", "morty", "api", "express", "nodejs"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "redis": "^4.6.0",
    "express-rate-limit": "^7.1.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.0",
    "cors": "^2.8.5",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "eslint": "^8.53.0",
    "supertest": "^6.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/config/*.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}
